{
  "name": "@ambrosus/react",
  "version": "0.13.0",
  "description": "Reusable react components for Ambrosus front-end apps.",
  "homepage": "http://ambrosus.github.io/react-components",
  "main": "./dist/index.js",
  "private": false,
  "author": "Ambrosus",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "https://github.com/ambrosus/react-components.git"
  },
  "scripts": {
    "start": "source ./.env.dev && webpack-dev-server --config ./webpack.config.dev.js",
    "docs": "source ./.env && webpack --config ./webpack.config.dev.js --mode=production",
    "build": "npm run build:cjs && npm run build:esm && npm run build:es && npm run build:umd",
    "build:cjs": "cross-env NODE_ENV=production BABEL_ENV=cjs babel --config-file ./babel.config.js ./src/components --out-dir ./build --extensions '.tsx'",
    "build:esm": "cross-env NODE_ENV=production BABEL_ENV=esm babel --config-file ./babel.config.js ./src/components --out-dir ./build/esm --extensions '.tsx'",
    "build:es": "cross-env NODE_ENV=production BABEL_ENV=es babel --config-file ./babel.config.js ./src/components --out-dir ./build/es --extensions '.tsx'",
    "build:umd": "cross-env BABEL_ENV=production-umd SASS_PATH=./node_modules:./src rollup -c ./rollup.config.js",
    "prebuild": "rimraf build",
    "release": "npm run build && npm publish build",
    "lint": "tslint -c tslint.json --project tsconfig.json 'src/**/*.{ts,tsx}'"
  },
  "dependencies": {
    "normalize.css": "^8.0.1",
    "prismjs": "^1.16.0",
    "react": "^16.8.5",
    "react-calendar": "^2.18.1",
    "react-dom": "^16.8.5",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.3.1",
    "react-scripts": "^2.1.3",
    "web3": "^1.0.0-beta.37"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.2.2",
    "@babel/plugin-proposal-class-properties": "^7.4.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.4.0",
    "@babel/plugin-transform-object-assign": "^7.2.0",
    "@babel/plugin-transform-runtime": "^7.4.0",
    "@babel/preset-env": "^7.2.3",
    "@babel/preset-react": "^7.0.0",
    "@babel/preset-typescript": "^7.3.3",
    "@types/html-webpack-plugin": "^3.2.0",
    "@types/prismjs": "^1.16.0",
    "@types/react": "^16.8.8",
    "@types/react-dom": "^16.8.3",
    "@types/react-router": "^4.4.5",
    "@types/react-router-dom": "^4.3.1",
    "@types/react-tabs": "^2.3.0",
    "@types/web3": "^1.0.14",
    "awesome-typescript-loader": "^5.2.1",
    "babel-core": "^6.26.3",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.5",
    "babel-plugin-react-remove-properties": "^0.3.0",
    "babel-plugin-transform-react-constant-elements": "^6.23.0",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.24",
    "case-sensitive-paths-webpack-plugin": "^2.1.2",
    "clean-webpack-plugin": "^1.0.0",
    "copy-webpack-plugin": "^4.6.0",
    "create-file-webpack": "^1.0.2",
    "cross-env": "^5.2.0",
    "css-loader": "^1.0.1",
    "file-loader": "^3.0.1",
    "html-webpack-plugin": "^3.2.0",
    "merge-files-webpack-plugin": "^1.1.2",
    "node-sass": "^4.11.0",
    "raw-loader": "^1.0.0",
    "rimraf": "^2.6.3",
    "rollup": "^1.8.0",
    "rollup-plugin-babel": "^4.3.2",
    "rollup-plugin-commonjs": "^9.2.3",
    "rollup-plugin-node-globals": "^1.4.0",
    "rollup-plugin-node-resolve": "^4.0.1",
    "rollup-plugin-postcss": "^2.0.3",
    "rollup-plugin-replace": "^2.1.1",
    "rollup-plugin-sass": "^1.2.2",
    "rollup-plugin-size-snapshot": "^0.8.0",
    "rollup-plugin-svg": "^1.0.1",
    "rollup-plugin-uglify": "^6.0.2",
    "sass-loader": "^7.1.0",
    "source-map-loader": "^0.2.4",
    "style-loader": "^0.23.1",
    "tslint": "^5.12.1",
    "tslint-config-airbnb": "^5.11.1",
    "tslint-config-prettier": "^1.17.0",
    "typescript": "^3.4.0-dev.20190319",
    "url-loader": "^1.1.2",
    "webpack": "^4.28.3",
    "webpack-cli": "^3.2.0",
    "webpack-concat-plugin": "^3.0.0",
    "webpack-dev-server": "^3.1.1"
  }
}
